nav(class="navbar" v-show="store.currentPage >= store.startNavbarPage && store.currentPage <= store.finishedPage - 1")
  ul.navbar__wrapper
    li(
      v-for="val, index in store.listNav" 
      class="navbar__item"
    )
      button(
        type="button" 
        :class="{active: store.currentPage - store.startNavbarPage == index }"
        @click="store.setNavbar(index)"
      ) 
        span {{val}}
